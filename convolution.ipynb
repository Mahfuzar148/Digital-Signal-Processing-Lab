    "import numpy as np\n",
    "import matplotlib.pyplot as plt \n",
    "\n",
    "n = np.arange(0,11)\n",
    "\n",
    "u_n = np.where(n>=0,1,0)\n",
    "un_5 = np.where(n>=5,1,0)\n",
    "\n",
    "x = np.copy(u_n)\n",
    "h_n = u_n-un_5\n",
    "\n",
    "y = np.convolve(u_n,h_n)\n",
    "y_n = np.arange(0,len(y))\n",
    "\n",
    "plt.stem(y_n,y)"
   ]
