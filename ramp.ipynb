   "source": [
    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "n= np.arange(-6,10)\n",
    "ramp = np.where(n>=0,n,0)\n",
    "\n",
    "plt.title(\"Ramp Signal\")\n",
    "plt.stem(n,ramp)\n",
    "plt.grid()\n",
    "plt.show()\n"
   ]
